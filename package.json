{
  "name": "vscode-collapse-whitespaces",
  "displayName": "Collapse Whitespaces",
  "description": "Helpers for collapsing whitespaces around the cursor in VSCode",
  "version": "0.0.1",
  "publisner": "calvinwyoung",
  "repository": "https://github.com/calvinwyoung/vscode-collapse-whitespaces",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-collapse-whitespaces.helloWorld",
    "onCommand:vscode-collapse-whitespaces.justOneSpace",
    "onCommand:vscode-collapse-whitespaces.collapseWhitespaces"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-collapse-whitespaces.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-collapse-whitespaces.justOneSpace",
        "title": "Just One Space"
      },
      {
        "command": "vscode-collapse-whitespaces.collapseWhitespaces",
        "title": "Collapse Whitespaces"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.45.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
